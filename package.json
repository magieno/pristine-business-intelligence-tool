{
  "name": "pristine-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.esm.json",
    "build:esm": "tsc -p tsconfig.json",
    "bundle": "rollup -c rollup.config.js --compact",
    "bundle:min": "rollup -c rollup.min.config.js --compact",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "express": "PRISTINE_LOGGING_CONSOLE_LOGGER_OUTPUT_MODE=SIMPLE ts-node ./src/express.ts",
    "local-sqs": "PRISTINE_LOGGING_CONSOLE_LOGGER_OUTPUT_MODE=SIMPLE ts-node ./src/local-sqs.ts"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.27.0",
    "@aws-sdk/client-sqs": "^3.27.0",
    "@awslabs-community-fork/dynamodb-data-mapper": "^0.7.13",
    "@awslabs-community-fork/dynamodb-data-mapper-annotations": "^0.7.13",
    "@pristine-ts/auth0": "^0.0.121",
    "@pristine-ts/aws": "^0.0.121",
    "@pristine-ts/aws-xray": "^0.0.121",
    "@pristine-ts/common": "^0.0.121",
    "@pristine-ts/configuration": "^0.0.121",
    "@pristine-ts/core": "^0.0.121",
    "@pristine-ts/event": "^0.0.121",
    "@pristine-ts/http": "^0.0.121",
    "@pristine-ts/jwt": "^0.0.121",
    "@pristine-ts/logging": "^0.0.121",
    "@pristine-ts/networking": "^0.0.121",
    "@pristine-ts/redis": "^0.0.121",
    "@pristine-ts/security": "^0.0.121",
    "@pristine-ts/stripe": "^0.0.121",
    "@pristine-ts/telemetry": "^0.0.121",
    "@pristine-ts/validation": "^0.0.121",
    "aws-lambda": "^1.0.6",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "date-fns": "^2.21.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@pristine-ts/express": "^0.0.121",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/aws-lambda": "^8.10.75",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "eslint": "^7.27.0",
    "jest": "^26.6.3",
    "node-polyfill-webpack-plugin": "^1.1.2",
    "rollup": "^2.56.2",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "strictNullChecks": false
        }
      }
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  }
}
